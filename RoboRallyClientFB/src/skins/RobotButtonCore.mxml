<?xml version="1.0" encoding="utf-8"?>
<s:Rect xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:mx="library://ns.adobe.com/flex/mx"
		width="100%"
		height="100%"
		radiusX="20"
		radiusY="20">
	<s:states>
		<s:State name="up" />
		<s:State name="over" />
		<s:State name="down" />
		<s:State name="disabled" />
	</s:states>
	<s:filters>
		<s:BevelFilter includeIn="up"
					   angle="45">

		</s:BevelFilter>
		<s:BevelFilter includeIn="down,over"
					   angle="225">

		</s:BevelFilter>
	</s:filters>
	<s:fill>
		<s:SolidColor color="0x323232">

		</s:SolidColor>
	</s:fill>
</s:Rect>
